{
    "compilerOptions": {
        "outDir": "./dist/",
        "jsx": "react-jsxdev",
        "module": "commonjs",
        "target": "es5",
        "esModuleInterop": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "sourceMap": true,
        "resolveJsonModule": true,
        "downlevelIteration": true,
        "strict": true,
        "baseUrl": "./",
        "paths": {
            "ui-components/*": ["lib/ui-components/*"],
            "utils/*": ["lib/utils/*"],
            "app/*": ["src/app/*"],
            "providers/*": ["src/api/providers/*"],
            "services/*": ["src/api/services/*"]
        }
    },
    "noUnusedLocals": true,
    "noUnusedParameters": true
}
